@using ISE309_ProjeOdev_B181200028_B181200039.Models.Sınıflar
@model List<Blog>
<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogImage" alt="" />
            </div>
            <div class="bot-desc">
                <p>@x.Aciklama.Substring(0,75)... </p>
            </div>
            <div class="clear"></div>
            
            
        }
    </div>
    @Html.Action("Partial5", "_deneme_")
</div>